<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="pj.rozkladWKD" android:versionName="1.2b" android:versionCode="13">

    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="4"/>
   
	
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
	
	<application android:icon="@drawable/ic_launcher_wkd" android:label="@string/app_name"
		android:description="@string/description" android:debuggable="true">

		<activity android:name=".RozkladWKD" android:label="@string/app_name"
			android:theme="@style/rozkladWKD_style">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="Info" android:theme="@style/rozkladWKD_style"
			android:label="@string/app_name" ></activity>
		<activity android:name="SearchResults" android:theme="@style/searchResults_style" android:configChanges="orientation"></activity>
		<activity android:name="SettingsActivity"></activity>
		<activity android:name="com.google.ads.AdActivity"
              android:configChanges="keyboard|keyboardHidden|orientation"/>
		<activity android:name="MessageActivity" android:theme="@style/rozkladWKD_style" android:configChanges="orientation"></activity>
	</application>

	

</manifest> 